<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.evelina.pooltraining">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icona"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ListaSettimana" />
        <activity android:name=".ListaEserciziActivity"
            android:label="Esercizi"/>
        <activity android:name=".ListaNuotatoriActivity"
            android:label="I miei nuotatori"/>
        <activity android:name=".AggiungiNuotatoreActivity"
            android:label="Nuotatori Liberi"/>
        <activity android:name=".AggiungiEserciziActivity"
            android:label ="Nuovo Esercizio"/>
        <activity android:name=".GiornoActivity"></activity>
    </application>

</manifest>